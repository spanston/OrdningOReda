@model TodoList

<div class="form-group col-12 col-md-3 float-left">

    <form asp-action="UndoLastRemovedItem" method="POST">
        <input type="hidden" name="Id" asp-for="Id" value="@ViewBag.ItemList.Id" />
        <button type="submit" class="btn btn-primary">Undo</button>

    </form>
</div>
